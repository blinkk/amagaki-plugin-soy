{
  "name": "@amagaki/amagaki-plugin-soy",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkkcode/amagaki-plugin-soy.git"
  },
  "engines": {
    "node": ">=13.14.0"
  },
  "scripts": {
    "build:example": "amagaki build example",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "dev:start": "npm run compile",
    "start": "amagaki serve example",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "fix": "node ./node_modules/eslint/bin/eslint --fix src/**/*.ts",
    "prepare": "rm -rf dist/; npm run dev:start;",
    "test": "ava"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/*.test.ts"
    ]
  },
  "dependencies": {
    "@amagaki/amagaki": "^0.2.0",
    "soynode": "^2.1.0",
    "@types/async": "^3.2.5",
    "@types/cli-progress": "^3.9.1",
    "@types/cli-table": "^0.3.0",
    "@types/express": "^4.17.11",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.0",
    "@types/marked": "^2.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.37",
    "@types/nunjucks": "^3.1.4",
    "@types/semver": "^7.3.4",
    "ava": "^3.15.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.7",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
